#!name= Picsart
#!desc= Picsart

[Header Rewrite]
^https:\/\/api\.(picsart|meiease)\.c(n|om)\/users\/show\/me\.json header-del if-none-match

[MITM]
hostname = %APPEND% buy.itunes.apple.com, api.picsart.com

[Script]
http-request https://api.picsart.com/gw-v2/shop/subscription/apple/purchases script-path=https://raw.githubusercontent.com/aleotoiday/patexiuto/master/Scripts/picsart.js, requires-body=true, timeout=5
