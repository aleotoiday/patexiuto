[Header Rewrite]
^https:\/\/api\.(picsart|meiease)\.c(n|om)\/users\/show\/me\.json header-del if-none-match
http-request ^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/user-customization-service\/v1\/customize$ header-del if-none-match

[MITM]
hostname = %APPEND% ap*.intsig.net, photos.adobe.io, api.revenuecat.com, nicegram.cloud, buy.itunes.apple.com, api.picsart.com, api-mobile.soundcloud.com, spclient.wg.spotify.com, *spclient.spotify.com, premium*.truecaller.com

