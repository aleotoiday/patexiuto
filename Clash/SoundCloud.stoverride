name: SoundCloud
desc: SoundCloud

http:
  mitm:
    - api-mobile.soundcloud.com

  script:
    - match: ^https://api-mobile.soundcloud.com/configuration/ios
      name: SoundCloudGo
      type: response
      require-body: true

script-providers:
  SoundCloudGo:
    url: https://raw.githubusercontent.com/aleotoiday/patexiuto/master/Scripts/soundcloud.js
    interval: 86400
