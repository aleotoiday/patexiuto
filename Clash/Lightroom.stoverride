name: Unlock Lightroom
desc: Lightroom

http:
  mitm:
    - photos.adobe.io

  script:
    - match: ^https://photos\.adobe\.io/v\d/accounts
      name: Lightroom
      type: response
      require-body: true
      max-size: 0

script-providers:
  Lightroom:
    url: https://raw.githubusercontent.com/aleotoiday/patexiuto/master/Scripts/lightroom.js
    interval: 86400
