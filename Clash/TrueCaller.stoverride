name: TrueCaller
desc: TrueCaller

http:
  mitm:
    - premium*.truecaller.com

  script:
    - match: ^https://premium-(.+)\.truecaller\.com/v\d/(subscriptions|products/apple)
      name: Truecaller
      type: response
      require-body: true
      max-size: 0

script-providers:
  Truecaller:
    url: https://raw.githubusercontent.com/aleotoiday/patexiuto/master/Scripts/truecaller.js
    interval: 86400
